{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "nodeVersion": "20.x",
        "supportsResponseStreaming": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}